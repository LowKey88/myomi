# Required Configuration
# ---------------------
# Redis connection settings
REDIS_DB_HOST=redis_container    # Redis host (use redis_container for docker-compose)
REDIS_DB_PORT=6379              # Redis port
REDIS_DB_PASSWORD=omipassword123 # Redis password (change in production)

# OpenAI API configuration
OPENAI_API_KEY=                 # Required for conversation feedback plugin

# Optional Integrations
# --------------------
# AskNews Integration
ASKNEWS_CLIENT_ID=             # Client ID for AskNews API access
ASKNEWS_CLIENT_SECRET=         # Client Secret for AskNews API access

# AI Model Integration
GROQ_API_KEY=                  # API key for Groq AI services

# Multion Integration
MULTION_API_KEY=               # API key for Multion services
MULTION_ORG_ID=                # Organization ID for Multion

# Mem0 Integration
MEM0_API_KEY=                  # API key for Mem0 services

# OAuth Configurations
# -------------------
# Notion Integration
NOTION_OAUTH_CLIENT_ID=        # Notion OAuth client ID
NOTION_OAUTH_CLIENT_SECRET=    # Notion OAuth client secret
NOTION_OAUTH_REDIRECT_URI=     # OAuth redirect URI for Notion
NOTION_AUTH_URL=              # Notion auth endpoint URL

# API URLs
# --------
FRIEND_API_URL=               # Base URL for Friend API
WORKFLOW_API_KEY=             # API key for workflow services
